<h2>We currently deliver to:</h2>
<style type="text/css">
    #map-canvas {
        height: 400px;
        width: 500px;
    }
</style>

<script type="text/javascript"
        src="http://www.google.com/jsapi?autoload={'modules':[{name:'maps',version:3,other_params:''}]}"></script>
<script type="text/javascript">
    function init() {
        var mapDiv = document.getElementById('map-canvas');
        var map = new google.maps.Map(mapDiv, {
            scrollwheel: false,
            draggable: false,
            zoom: 11,
            center: new google.maps.LatLng(44.969469, -93.414132),
            mapTypeId: google.maps.MapTypeId.ROADMAP
        });

        var deliveryLocationsCoords = [
            new google.maps.LatLng(45.018998, -93.482454),
            new google.maps.LatLng(45.009776, -93.342035),
            new google.maps.LatLng(44.932051, -93.354051),
            new google.maps.LatLng(44.908713, -93.509233)
        ];

        var deliveryLocation = new google.maps.Polygon({
            paths: deliveryLocationsCoords,
            strokeColor: '#FF0000',
            strokeOpacity: 0.8,
            strokeWeight: 2,
            fillColor: '#FF0000',
            fillOpacity: 0.35
        });

        deliveryLocation.setMap(map);
    }

    google.maps.event.addDomListener(window, 'load', init);
</script>
<div id="map-canvas"></div>